// Place your key bindings in this file to override the defaults for vs-code
// 如果和系统快捷键冲突，可通过 gsettings get org.gnome.desktop.wm.keybindings switch-to-workspace-left获得系统快捷键, 
// gsettings set org.gnome.desktop.wm.keybindings switch-to-workspace-left ['<Super><Alt>Left']设置系统快捷键
[
    {
        "key": "ctrl+alt+left",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "ctrl+alt+right",
        "command": "workbench.action.navigateForward"
    },
    {
        "key": "ctrl+alt+up",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+alt+down",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+down",
        "command": "editor.action.insertCursorBelow",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+up",
        "command": "editor.action.insertCursorAbove",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+y",
        "command": "editor.action.deleteLines",
        "when": "textInputFocus && !editorReadonly"
    },
]
